<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DeviceProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DeviceProperties_V08.xsd">
    <Device>
        <name implementation="CSV2SerialAdapter">CSV2Serial2</name>
        <manufacturer>WB</manufacturer>
        <manufacturerURL>http://www.bruegmaenner.de/OpenSerialDataExplorer</manufacturerURL>
        <group>multimeter</group>
        <image>CSV2SerialAdapter.png</image>
        <usage>false</usage>
    </Device>
    <TimeBase>
        <name>Time</name>
        <symbol>t</symbol>
        <unit>msec</unit>
        <timeStep>-1.0</timeStep>
    </TimeBase>
    <DataBlock>
        <format ending="0D0A" separator="semicolon" timeUnit="msec" size="1" type="TEXT"/>
        <preferredDataLocation>d:\Documents\Person\OpenSerialDataExplorer\CSV2SerialDataAdapter</preferredDataLocation>
        <preferredFileExtention>*.csv</preferredFileExtention>
    </DataBlock>
    <State>
        <property description="" type="Integer" value="01" name="Datenaufzeichnung"/>
    </State>
    <Channel type="TYPE_CONFIG" name="Thermik">
        <Measurement>
            <name>Spannung</name>
            <symbol>Ue</symbol>
            <unit>V</unit>
            <active>true</active>
            <statistics sigma="true" avg="true" max="true" min="true"/>
            <property description="Faktor * input value" type="Double" value="0.001" name="factor"/>
        </Measurement>
    </Channel>
    <Desktop>
        <property type="Boolean" value="true" name="table_tab"/>
        <property type="Boolean" value="true" name="digital_tab"/>
        <property type="Boolean" value="true" name="analog_tab"/>
        <property type="Boolean" value="true" name="voltage_per_cell_tab"/>
    </Desktop>
</DeviceProperties>
