n="Charge processing description" value="1" name="charge"/>		<property type="Integer" description="Discharge processing description" value="2" name="discharge"/>	</State>	<Channels>		<Channel type="TYPE_OUTLET" name="Outlet">			<Measurement>				<name>Voltage</name>				<symbol>U</symbol>				<unit>V</unit>				<active>true</active>				<statistics sigma="false" avg="false" max="true" min="true"/>			</Measurement>			<Measurement>				<name>Current</name>				<symbol>I</symbol>				<unit>A</unit>				<active>true</active>				<statistics sigma="true" avg="true" max="true" min="true"/>			</Measurement>			<Measurement>				<name>Capacity</name>				<symbol>C</symbol>				<unit>mAh</unit>				<active>true</active>				<statistics sigma="false" avg="false" max="true" min="true"/>			</Measurement>			<Measurement>				<name>Power</name>				<symbol>P</symbol>				<unit>W</unit>				<statistics sigma="false" avg="false" max="true" min="true"/>			</Measurement>			<Measurement>				<name>Energy</name>				<symbol>E</symbol>				<unit>Wh</unit>				<statistics sigma="false" avg="false" max="true" min="true"/>			</Measurement>		</Channel>	</Channels>	<Desktop>		<property value="true" name="table_tab"/>		<property value="true" name="digital_tab"/>		<property value="true" name="analog_tab"/>		<property value="false" name="voltage_per_cell_tab"/>	</Desktop></DeviceProperties>