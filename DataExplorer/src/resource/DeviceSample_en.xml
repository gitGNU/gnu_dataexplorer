<?xml version="1.0" encoding="UTF-8"?><DeviceProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DeviceProperties_V10.xsd">	<Device>		<name>SampleDevice</name>		<manufacturer>XY-Coporation</manufacturer>		<manufacturerURL>http://www.sample.com</manufacturerURL>		<group>logger</group>		<image>sample.jpg</image>		<usage>false</usage>	</Device>	<SerialPort>		<port>COM2</port>		<baudeRate>9600</baudeRate>		<dataBits>DATABITS_8</dataBits>		<stopBits>STOPBITS_1</stopBits>		<parity>PARITY_NONE</parity>		<flowControlMode>FLOWCONTROL_NONE</flowControlMode>		<isRTS>false</isRTS>		<isDTR>false</isDTR>	</SerialPort>	<TimeBase>		<name>Time</name>		<symbol>t</symbol>		<unit>msec</unit>		<timeStep>1</timeStep>	</TimeBase>	<DataBlock>        <format size="25" type="BINARY"/>		<checkSum type="ADD" format="BINARY"/>		<timeUnit>sec</timeUnit>		<separator>semicolon</separator>		<leader>$</leader>		<trailer>0D0A</trailer>	</DataBlock>	<State>		<property type="Integer" description="Charge processing description" value="1" name="charge"/>		<property type="Integer" description="Discharge processing description" value="2" name="discharge"/>	</State>	<Channel type="TYPE_OUTLET" name="Outlet">		<Measurement>			<name>Voltage</name>			<symbol>U</symbol>			<unit>V</unit>			<active>true</active>			<statistics sigma="false" avg="false" max="true" min="true"/>		</Measurement>		<Measurement>			<name>Current</name>			<symbol>I</symbol>			<unit>A</unit>			<active>true</active>			<statistics sigma="true" avg="true" max="true" min="true"/>		</Measurement>		<Measurement>			<name>Capacity</name>			<symbol>C</symbol>			<unit>mAh</unit>			<active>true</active>			<statistics sigma="false" avg="false" max="true" min="true"/>		</Measurement>		<Measurement>			<name>Power</name>			<symbol>P</symbol>			<unit>W</unit>			<statistics sigma="false" avg="false" max="true" min="true"/>		</Measurement>		<Measurement>			<name>Energy</name>			<symbol>E</symbol>			<unit>Wh</unit>			<statistics sigma="false" avg="false" max="true" min="true"/>		</Measurement>	</Channel>	<Desktop>		<property type="Boolean" value="true" name="table_tab"/>		<property type="Boolean" value="true" name="digital_tab"/>		<property type="Boolean" value="true" name="analog_tab"/>		<property type="Boolean" value="false" name="voltage_per_cell_tab"/>	</Desktop></DeviceProperties>