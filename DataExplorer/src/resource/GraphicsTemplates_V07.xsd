on value="RecordSet_horizontalGridColor"/>						<xs:enumeration value="RecordSet_horizontalGridLineStyle"/>						<xs:enumeration value="RecordSet_horizontalGridRecordOrdinal"/>						<xs:enumeration value="RecordSet_horizontalGridType"/>						<xs:enumeration value="RecordSet_timeGridColor"/>						<xs:enumeration value="RecordSet_timeGridLineStyle"/>						<xs:enumeration value="RecordSet_timeGridType"/>					</xs:restriction>				</xs:simpleType>			</xs:attribute>		</xs:complexType>	</xs:element>	<xs:element name="properties">		<xs:complexType>			<xs:sequence>				<xs:element ref="comment"/>				<xs:element ref="entry" maxOccurs="unbounded"/>			</xs:sequence>		</xs:complexType>	</xs:element></xs:schema>