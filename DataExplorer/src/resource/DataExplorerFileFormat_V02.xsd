nnotation>					<xs:documentation>Pointer is offset from beginniing to enable asynchronous read</xs:documentation>				</xs:annotation>			</xs:element>		</xs:sequence>	</xs:complexType>	<xs:complexType name="DataExplorerFileFormatType">		<xs:sequence>			<xs:element name="VersionStamp" type="xs:string"/>			<xs:element name="CreationStamp" type="xs:dateTime"/>			<xs:element name="FileComment" type="xs:string" minOccurs="0"/>			<xs:element name="DeviceName" type="xs:string"/>			<xs:element name="NumberOfRecordSets" type="xs:integer"/>			<xs:element name="ObjectKey" type="xs:string" minOccurs="0"/>			<xs:element name="RecordSetDescription" type="RecordSetDescriptionType" maxOccurs="unbounded"/>			<xs:element name="RecordSetDataPoints" maxOccurs="unbounded">				<xs:complexType>					<xs:sequence>						<xs:element name="RecordDataArray" type="xs:hexBinary" maxOccurs="unbounded"/>					</xs:sequence>				</xs:complexType>			</xs:element>		</xs:sequence>	</xs:complexType>	<xs:complexType name="ColorType">		<xs:sequence>			<xs:element name="Red" type="xs:integer"/>			<xs:element name="Green" type="xs:integer"/>			<xs:element name="Blue" type="xs:integer"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="MeasurementType">		<xs:sequence>			<xs:element name="name" type="xs:string"/>			<xs:element name="symbol" type="xs:string"/>			<xs:element name="unit" type="xs:string"/>			<xs:element name="active" type="xs:boolean" nillable="false" minOccurs="0">				<xs:annotation>					<xs:documentation>true can read data, false inactive, null -> calculation = true</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="Property" type="PropertyType" minOccurs="0" maxOccurs="unbounded"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="PropertyType">		<xs:attribute name="name" type="xs:string" use="required"/>		<xs:attribute name="value" use="required"/>		<xs:attribute name="type" type="data_types" use="required"/>		<xs:attribute name="description" type="xs:string"/>	</xs:complexType>	<xs:simpleType name="data_types">		<xs:restriction base="xs:string">			<xs:enumeration value="Integer"/>			<xs:enumeration value="Double"/>			<xs:enumeration value="Boolean"/>			<xs:enumeration value="String"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="line_style_type">		<xs:restriction base="xs:string">			<xs:enumeration value="LINE_SOLID"/>			<xs:enumeration value="LINE_DASH"/>			<xs:enumeration value="LINE_DOT"/>			<xs:enumeration value="LINE_DASHDOT"/>			<xs:enumeration value="LINE_DASHDOTDOT"/>			<xs:enumeration value="LINE_CUSTOM"/>		</xs:restriction>	</xs:simpleType>	<xs:complexType name="RecordPropertiesType">		<xs:sequence>			<xs:element name="Measurement" type="MeasurementType"/>			<xs:element name="DisplayProperties" minOccurs="0">				<xs:complexType>					<xs:sequence>						<xs:element name="isDisplayable" type="xs:boolean" minOccurs="0"/>						<xs:element name="isVisible" type="xs:boolean" minOccurs="0"/>						<xs:element name="maxValue" type="xs:integer" minOccurs="0"/>						<xs:element name="minValue" type="xs:integer" minOccurs="0"/>						<xs:element name="defMaxValue" type="xs:double" minOccurs="0"/>						<xs:element name="defMinValue" type="xs:double" minOccurs="0"/>						<xs:element name="isPositionLeft" type="xs:boolean" minOccurs="0"/>						<xs:element name="color" type="ColorType" minOccurs="0"/>						<xs:element name="lineWidth" type="xs:integer" minOccurs="0"/>						<xs:element name="lineStyle" type="line_style_type" minOccurs="0"/>						<xs:element name="isRoundOut" type="xs:boolean" minOccurs="0"/>						<xs:element name="isStartpointZero" type="xs:boolean" minOccurs="0"/>						<xs:element name="isStartEndDefined" type="xs:boolean" minOccurs="0"/>						<xs:element name="numberFormat" type="xs:integer" minOccurs="0"/>					</xs:sequence>				</xs:complexType>			</xs:element>		</xs:sequence>	</xs:complexType>	<xs:complexType name="RecordSetPropertiesType">		<xs:sequence>			<xs:element name="TimeStep_ms" type="xs:double"/>			<xs:element name="TimeGridType" type="xs:integer" minOccurs="0"/>			<xs:element name="TimeGridLineStyle" type="line_style_type" minOccurs="0"/>			<xs:element name="TimeGridColor" type="ColorType" minOccurs="0"/>			<xs:element name="HorizontalGridRecord" type="xs:string" minOccurs="0"/>			<xs:element name="HorizontalGridType" type="xs:integer" minOccurs="0"/>			<xs:element name="HorizontalGridLineStyle" type="line_style_type" minOccurs="0"/>			<xs:element name="HorizontalGridColor" type="ColorType" minOccurs="0"/>		</xs:sequence>	</xs:complexType></xs:schema>