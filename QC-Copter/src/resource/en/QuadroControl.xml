<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DeviceProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DeviceProperties_V10.xsd">
    <Device>
        <name implementation="gde.device.tttronix.QcCopter">QuadroControl</name>
        <manufacturer>tt-tronix</manufacturer>
        <manufacturerURL>http://www.qc-copter.de</manufacturerURL>
        <group>multimeter</group>
        <image>QuadroControl.png</image>
        <usage>false</usage>
    </Device>
    <SerialPort>
        <port>-</port>
        <baudeRate>57600</baudeRate>
        <dataBits>DATABITS_8</dataBits>
        <stopBits>STOPBITS_1</stopBits>
        <parity>PARITY_NONE</parity>
        <flowControlMode>FLOWCONTROL_RTSCTS_OUT</flowControlMode>
        <isRTS>false</isRTS>
        <isDTR>false</isDTR>
    </SerialPort>
    <TimeBase>
        <name>Time</name>
        <symbol>t</symbol>
        <unit>msec</unit>
        <timeStep>0.0</timeStep>
    </TimeBase>
    <DataBlock>
        <format size="62" type="BINARY"/>
        <checkSum type="ADD" format="BINARY"/>
    </DataBlock>
    <State>
        <property type="Integer" value="1" name="simulation"/>
    </State>
    <Channel type="TYPE_CONFIG" name="config">
        <Measurement>
            <name>Roll-Integral</name>
            <symbol>Ri</symbol>
            <unit>°</unit>
            <active>true</active>
            <statistics sigma="true" avg="true" max="true" min="true"/>
            <property description="Factor to measurement value" type="Double" value="1000.0" name="factor"/>
        </Measurement>
        <Measurement>
            <name>Roll-Winkel</name>
