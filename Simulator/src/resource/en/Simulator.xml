<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DeviceProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DeviceProperties_V10.xsd">
    <Device>
        <name>Simulator</name>
        <manufacturer>WB</manufacturer>
        <manufacturerURL>http://www.bruegmaenner.de/en/winfried</manufacturerURL>
        <group>multimeter</group>
        <image>Simulator.jpg</image>
        <usage>false</usage>
    </Device>
    <TimeBase>
        <name>Time</name>
        <symbol>t</symbol>
        <unit>msec</unit>
        <timeStep>5</timeStep>
    </TimeBase>
    <DataBlock>
        <format size="100" type="BINARY"/>
    </DataBlock>
    <Channel type="TYPE_CONFIG" name="Sinus">
        <Measurement>
            <name>Voltage</name>
            <symbol>U</symbol>
            <unit>V</unit>
            <active>true</active>
			<statistics min="true" max="true" avg="true" sigma="true"/>
        </Measurement>
        <Measurement>
            <name>Height</name>
            <symbol>H</symbol>
            <unit>m</unit>
            <active>true</active>
			<statistics min="true" max="true" avg="true" sigma="true"/>
        </Measurement>
    </Channel>
    <Channel type="TYPE_OUTLET" name="Ramp">
        <Measurement>
            <name>Voltage</name>
            <symbol>U</symbol>
            <unit>V</unit>
            <active>true</active>
        </Measurement>
        <Measurement>
            <name>Current</name>
            <symbol>I</symbol>
            <unit>A</unit>
            <active>true</active>
        </Measurement>
    </Channel>
    <Desktop>
		<property type="Boolean" value="false" name="table_tab"/>
		<property type="Boolean" value="false" name="voltage_per_cell_tab"/>
		<property type="Boolean" value="false" name="analog_tab"/>
		<property type="Boolean" value="false" name="digital_tab"/>
    </Desktop>
</DeviceProperties>
