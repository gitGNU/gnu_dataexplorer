<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DeviceProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DeviceProperties_V06.xsd">
    <Device>
        <name>Simulator</name>
        <manufacturer>WB</manufacturer>
		<manufacturerURL/>
        <group>multimeter</group>
        <image>Simulator.jpg</image>
        <usage>false</usage>
    </Device>
    <SerialPort>
        <port>COM3</port>
        <baudeRate>38400</baudeRate>
        <dataBits>8</dataBits>
        <stopBits>STOPBITS_1</stopBits>
        <parity>PARITY_NONE</parity>
        <flowControlMode>FLOWCONTROL_NONE</flowControlMode>
        <isRTS>false</isRTS>
        <isDTR>false</isDTR>
        <DataBlock>
            <size>50</size>
        </DataBlock>
    </SerialPort>
    <TimeBase>
        <name>Time</name>
        <symbol>t</symbol>
        <unit>ms</unit>
        <timeStep>5</timeStep>
    </TimeBase>
    <Channel type="TYPE_CONFIG" name="Sinus">
        <Measurement>
            <name>Voltage</name>
            <symbol>U</symbol>
            <unit>V</unit>
            <active>true</active>
			<statistics min="true" max="true" avg="true" sigma="true"/>
        </Measurement>
        <Measurement>
            <name>Height</name>
            <symbol>H</symbol>
            <unit>m</unit>
            <active>true</active>
			<statistics min="true" max="true" avg="true" sigma="true"/>
        </Measurement>
    </Channel>
    <Channel type="TYPE_OUTLET" name="SÃ¤gezahn">
        <Measurement>
            <name>Voltage</name>
            <symbol>U</symbol>
            <unit>V</unit>
            <active>true</active>
        </Measurement>
        <Measurement>
            <name>Current</name>
            <symbol>I</symbol>
            <unit>A</unit>
            <active>true</active>
        </Measurement>
    </Channel>
    <Desktop>
		<property type="Boolean" value="false" name="table_tab"/>
		<property type="Boolean" value="false" name="voltage_per_cell_tab"/>
		<property type="Boolean" value="false" name="analog_tab"/>
		<property type="Boolean" value="false" name="digital_tab"/>
    </Desktop>
</DeviceProperties>
