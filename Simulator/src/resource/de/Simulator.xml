<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DeviceProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DeviceProperties_V18.xsd">
    <Device>
        <name>Simulator</name>
        <manufacturer>WB</manufacturer>
        <manufacturerURL>http://www.bruegmaenner.de/de/winfried</manufacturerURL>
        <group>multimeter</group>
        <image>Simulator.jpg</image>
        <usage>false</usage>
    </Device>
    <TimeBase>
        <name>Zeit</name>
        <symbol>t</symbol>
        <unit>msec</unit>
        <timeStep>5</timeStep>
    </TimeBase>
    <DataBlock>
        <format inputType="SERIAL_IO" formatType="BYTE" size="100"/>
    </DataBlock>
    <Channels>
	    <Channel type="TYPE_CONFIG" name="Sinus">
	        <Measurement>
	            <name>Spannung</name>
	            <symbol>U</symbol>
	            <unit>V</unit>
	            <active>true</active>
				<statistics min="true" max="true" avg="true" sigma="true"/>
	        </Measurement>
	        <Measurement>
	            <name>Höhe</name>
	            <symbol>H</symbol>
	            <unit>m</unit>
	            <active>true</active>
				<statistics min="true" max="true" avg="true" sigma="true"/>
	        </Measurement>
	    </Channel>
	    <Channel type="TYPE_OUTLET" name="Sägezahn">
	        <Measurement>
	            <name>Spannung</name>
	            <symbol>U</symbol>
	            <unit>V</unit>
	            <active>true</active>
	        </Measurement>
	        <Measurement>
	            <name>Strom</name>
	            <symbol>I</symbol>
	            <unit>A</unit>
	            <active>true</active>
	        </Measurement>
	    </Channel>
	</Channels>
    <Desktop>
		<property value="false" name="table_tab"/>
		<property value="false" name="voltage_per_cell_tab"/>
		<property value="false" name="analog_tab"/>
		<property value="false" name="digital_tab"/>
    </Desktop>
</DeviceProperties>
