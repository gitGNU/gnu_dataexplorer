<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DeviceProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DeviceProperties_V31.xsd">
    <Device>
        <name>LiPoWatch</name>
        <manufacturer>SM-Modellbau</manufacturer>
        <manufacturerURL>http://www.sm-modellbau.de</manufacturerURL>
        <group>logger</group>
        <image>LiPoWatch.jpg</image>
        <usage>false</usage>
    </Device>
    <SerialPort>
        <port>-</port>
        <baudeRate>115200</baudeRate>
        <dataBits>DATABITS_8</dataBits>
        <stopBits>STOPBITS_1</stopBits>
        <parity>PARITY_NONE</parity>
        <flowControlMode>FLOWCONTROL_NONE</flowControlMode>
        <isRTS>true</isRTS>
        <isDTR>true</isDTR>
    </SerialPort>
    <TimeBase>
        <name>Zeit</name>
        <symbol>t</symbol>
        <unit>msec</unit>
        <timeStep>500</timeStep>
    </TimeBase>
    <DataBlock>
        <format inputType="SERIAL_IO" formatType="BYTE" size="-76"/>
		<checkSum type="XOR" format="BINARY"/>
    </DataBlock>
    <Channels>
	    <Channel type="TYPE_OUTLET" name="Konfiguration">
	        <Measurement>
	            <name>Gesamtspannung</name>
	            <symbol>Ut</symbol>
	            <unit>V</unit>
	            <active>true</active>
	            <statistics sigma="true" avg="true" max="true" min="true"/>
	        </Measurement>
	        <Measurement>
	            <name>ServoImpuls_ein</name>
	            <symbol>SIe</symbol>
	            <unit>µs</unit>
	            <active>true</active>
	            <statistics sigma="false" avg="false" max="true" min="true"/>
	        </Measurement>
	        <Measurement>
	            <name>ServoImpuls_aus</name>
	            <symbol>SIa</symbol>
	            <unit>µs</unit>
	            <active>true</active>
	            <statistics sigma="false" avg="false" max="true" min="true"/>
	            <property type="Integer" value="1" name="scale_sync_ref_ordinal"/>
	        </Measurement>
	        <Measurement>
	            <name>Temperatur</name>
	            <symbol>A1</symbol>
	            <unit>°C</unit>
	            <active>true</active>
	            <statistics sigma="true" avg="true" max="true" min="true"/>
	            <property type="Double" value="0.0" name="offset"/>
	            <property type="Double" value="1.0" name="factor"/>
	        </Measurement>
	        <Measurement>
	            <name>Balance</name>
	            <symbol>Bal</symbol>
	            <unit>mV</unit>
	            <statistics sigma="true" avg="true" max="true" min="true"/>
	            <property description="Faktor zu einem gemessenem Wert" type="Double" value="1000.0" name="factor"/>
	        </Measurement>
