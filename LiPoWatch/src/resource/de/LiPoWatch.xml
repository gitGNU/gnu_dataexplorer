<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DeviceProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DeviceProperties_V07.xsd">
    <Device>
        <name>LiPoWatch</name>
        <manufacturer>SM-Modellbau</manufacturer>
        <manufacturerURL>http://www.sm-modellbau.de</manufacturerURL>
        <group>logger</group>
        <image>LiPoWatch.jpg</image>
        <usage>false</usage>
    </Device>
    <SerialPort>
        <port>COM10</port>
        <baudeRate>115200</baudeRate>
        <dataBits>8</dataBits>
        <stopBits>STOPBITS_1</stopBits>
        <parity>PARITY_NONE</parity>
        <flowControlMode>FLOWCONTROL_NONE</flowControlMode>
        <isRTS>true</isRTS>
        <isDTR>true</isDTR>
        <DataBlock>
            <size>24</size>
            <checkSum>XOR</checkSum>
        </DataBlock>
    </SerialPort>
    <TimeBase>
        <name>Zeit</name>
        <symbol>t</symbol>
        <unit>ms</unit>
        <timeStep>500</timeStep>
    </TimeBase>
    <Channel type="TYPE_OUTLET" name="Konfiguration">
        <Measurement>
            <name>Gesamtspannung</name>
            <symbol>Ut</symbol>
            <unit>V</unit>
            <active>true</active>
            <statistics sigma="true" avg="true" max="true" min="true"/>
        </Measurement>
        <Measurement>
            <name>ServoImpuls_ein</name>
            <symbol>SIe</symbol>
            <unit>µs</unit>
            <active>true</active>
            <statistics sigma="false" avg="false" max="true" min="true"/>
        </Measurement>
        <Measurement>
            <name>ServoImpuls_aus</name>
            <symbol>SIa</symbol>
            <unit>µs</unit>
            <active>true</active>
            <statistics sigma="false" avg="false" max="true" min="true"/>
        </Measurement>
        <Measurement>
            <name>Temperatur</name>
            <symbol>A1</symbol>
            <unit>°C</unit>
            <active>true</active>
            <statistics sigma="true" avg="true" max="true" min="true"/>
            <property type="Double" value="0.0" name="offset"/>
            <property type="Double" value="1.0" name="factor"/>
        </Measurement>
        <Measurement>
            <name>Zellenspannung 1</name>
