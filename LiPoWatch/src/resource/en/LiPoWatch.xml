<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DeviceProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DeviceProperties_V11.xsd">
    <Device>
        <name>LiPoWatch</name>
        <manufacturer>SM-Modellbau</manufacturer>
        <manufacturerURL>http://www.sm-modellbau.de</manufacturerURL>
        <group>logger</group>
        <image>LiPoWatch.jpg</image>
        <usage>false</usage>
    </Device>
    <SerialPort>
        <port>-</port>
        <baudeRate>115200</baudeRate>
        <dataBits>DATABITS_8</dataBits>
        <stopBits>STOPBITS_1</stopBits>
        <parity>PARITY_NONE</parity>
        <flowControlMode>FLOWCONTROL_NONE</flowControlMode>
        <isRTS>true</isRTS>
        <isDTR>true</isDTR>
    </SerialPort>
    <TimeBase>
        <name>Time</name>
        <symbol>t</symbol>
        <unit>msec</unit>
        <timeStep>500</timeStep>
    </TimeBase>
    <DataBlock>
        <format type="BINARY" size="-76"/>
		<checkSum type="XOR" format="BINARY"/>
    </DataBlock>
    <Channel type="TYPE_OUTLET" name="Configuration">
        <Measurement>
            <name>TotalVoltage</name>
            <symbol>Ut</symbol>
            <unit>V</unit>
            <active>true</active>
            <statistics sigma="true" avg="true" max="true" min="true"/>
        </Measurement>
        <Measurement>
            <name>ServoImpuls_in</name>
            <symbol>SIi</symbol>
            <unit>µs</unit>
            <active>true</active>
            <statistics sigma="false" avg="false" max="true" min="true"/>
        </Measurement>
        <Measurement>
            <name>ServoImpuls_out</name>
            <symbol>SIo</symbol>
            <unit>µs</unit>
            <active>true</active>
            <statistics sigma="false" avg="false" max="true" min="true"/>
        </Measurement>
        <Measurement>
            <name>Temperature</name>
            <symbol>A1</symbol>
            <unit>°C</unit>
            <active>true</active>
            <statistics sigma="true" avg="true" max="true" min="true"/>
            <property type="Double" value="0.0" name="offset"/>
            <property type="Double" value="1.0" name="factor"/>
        </Measurement>
        <Measurement>
