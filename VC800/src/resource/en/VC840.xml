<?xml version="1.0" encoding="UTF-8"?><DeviceProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DeviceProperties_V08.xsd">	<Device>		<name>VC840</name>		<manufacturer>Conrad</manufacturer>		<manufacturerURL>http://www.conrad.de</manufacturerURL>		<group>MultiMeter</group>		<image>VC8X0.jpg</image>		<usage>false</usage>	</Device>	<SerialPort>		<port>COM2</port>		<baudeRate>2400</baudeRate>		<dataBits>8</dataBits>		<stopBits>STOPBITS_1</stopBits>		<parity>PARITY_NONE</parity>		<flowControlMode>FLOWCONTROL_NONE</flowControlMode>		<isRTS>false</isRTS>		<isDTR>true</isDTR>	</SerialPort>	<TimeBase>		<name>Time</name>		<symbol>t</symbol>		<unit>msec</unit>		<timeStep>-1</timeStep>	</TimeBase>    <DataBlock>		<format>BINARY</format>        <size>14</size>        <ending>13</ending>    </DataBlock>	<Channel type="TYPE_OUTLET" name="Outlet">		<Measurement>			<name>Voltage</name>			<symbol>U</symbol>			<unit>V</unit>			<active>true</active>			<statistics min="true" max="true" avg="true" sigma="true"/>		</Measurement>	</Channel>    <Desktop>        <property type="Boolean" value="false" name="table_tab"/>        <property type="Boolean" value="true" name="digital_tab"/>        <property type="Boolean" value="true" name="analog_tab"/>        <property type="Boolean" value="false" name="voltage_per_cell_tab"/>    </Desktop></DeviceProperties>