<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DeviceProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DeviceProperties_V28.xsd">
    <Device>
        <name>JivePro</name>
        <manufacturer>Kontronik</manufacturer>
        <manufacturerURL>http://www.kontronik.de</manufacturerURL>
        <group>logger</group>
        <image>JivePro.jpg</image>
        <usage>false</usage>
    </Device>
    <SerialPort>
        <port></port>
        <baudeRate>9600</baudeRate>
        <dataBits>DATABITS_8</dataBits>
        <stopBits>STOPBITS_1</stopBits>
        <parity>PARITY_NONE</parity>
        <flowControlMode>FLOWCONTROL_NONE</flowControlMode>
        <isRTS>false</isRTS>
        <isDTR>false</isDTR>
    </SerialPort>
    <TimeBase>
        <name>Time</name>
        <symbol>t</symbol>
        <unit>msec</unit>
        <timeStep>-1.0</timeStep>
    </TimeBase>
    <DataBlock>
        <format size="-200" formatType="BYTE" inputType="SERIAL_IO"/>
        <format size="-14" formatType="VALUE" inputType="FILE_IO"/>
        <timeUnit>sec</timeUnit>
        <separator>semicolon</separator>
        <leader>$</leader>
        <trailer>0D0A</trailer>
        <preferredFileExtention>*.dat</preferredFileExtention>
    </DataBlock>
    <State>
        <property description="" type="Integer" value="01" name="Aufzeichnung"/>
    </State>
    <Channels lastUseOrdinal="1">
       <Channel type="TYPE_OUTLET" name="Konfig 1">
            <Measurement>
                <name>Drehzahl</name>
                <symbol>rpm</symbol>
                <unit>1/min</unit>
                <active>true</active>
                <statistics sigma="false" avg="true" max="true" min="true"/>
            </Measurement>
			<Measurement>
                <name>Spannung Akku</name>
				<symbol>Ubat</symbol>
				<unit>V</unit>
				<active>true</active>
                <statistics sigma="false" avg="true" max="true" min="true"/>
			</Measurement>
            <Measurement>
                <name>Strom Akku</name>
                <symbol>Ibat</symbol>
                <unit>A</unit>
                <active>true</active>
                <statistics sigma="false" avg="true" max="true" min="true"/>
            </Measurement>
            <Measurement>
                <name>Kapazit채t Akku</name>
                <symbol>C</symbol>
                <unit>mAh</unit>
                <active>true</active>
                <statistics sigma="false" avg="true" max="true" min="true"/>
                <property description="Faktor zu einem gemessenem Wert" type="Double" value="1000.0" name="factor"/>
            </Measurement>
            <Measurement>
                <name>Strom Motor</name>
                <symbol>Imot</symbol>
                <unit>A</unit>
                <active>true</active>
                <statistics sigma="false" avg="true" max="true" min="true"/>
                <property name="scale_sync_ref_ordinal" value="2" type="Integer" description="Skalasynchronisations Messwertreferenz ala Zahl"/>
            </Measurement>
            <Measurement>
                <name>Strom Motor(peak)</name>
                <symbol>Imot</symbol>
                <unit>A</unit>
                <active>true</active>
                <statistics sigma="false" avg="true" max="true" min="true"/>
                <property name="scale_sync_ref_ordinal" value="2" type="Integer" description="Skalasynchronisations Messwertreferenz ala Zahl"/>
            </Measurement>
            <Measurement>
                <name>Temperatur FET</name>
                <symbol>Tfet</symbol>
                <unit>째C</unit>
                <active>true</active>
                <statistics sigma="false" avg="true" max="true" min="true"/>
            </Measurement>
            <Measurement>
                <name>PWM</name>
                <symbol></symbol>
                <unit>%</unit>
                <active>true</active>
                <statistics sigma="false" avg="true" max="true" min="true"/>
            </Measurement>
			<Measurement>
				<name>Spannung BEC</name>
				<symbol>Ubec</symbol>
				<unit>V</unit>
				<active>true</active>
                <statistics sigma="true" avg="true" max="true" min="true"/>
			</Measurement>
			<Measurement>
				<name>Strom BEC</name>
				<symbol>Ibec</symbol>
				<unit>A</unit>
				<active>true</active>
                <statistics sigma="true" avg="true" max="true" min="true"/>
			</Measurement>
            <Measurement>
                <name>Temperatur BEC</name>
                <symbol>T</symbol>
                <unit>째C</unit>
                <active>true</active>
                <statistics sigma="false" avg="true" max="true" min="true"/>
                <property name="scale_sync_ref_ordinal" value="6" type="Integer" description="Skalasynchronisations Messwertreferenz ala Zahl"/>
            </Measurement>
            <Measurement>
                <name>Gas (in)</name>
                <symbol>thr</symbol>
                <unit>%</unit>
                <active>true</active>
                <statistics sigma="true" avg="true" max="true" min="true"/>
            </Measurement>
            <Measurement>
                <name>Gas (eff)</name>
                <symbol>thr</symbol>
                <unit>%</unit>
                <active>true</active>
                <statistics sigma="true" avg="true" max="true" min="true"/>
                <property name="scale_sync_ref_ordinal" value="11" type="Integer" description="Skalasynchronisations Messwertreferenz ala Zahl"/>
            </Measurement>         									
            <Measurement>
                <name>Timing</name>
                <symbol>tmg</symbol>
                <unit>째</unit>
                <active>true</active>
                <statistics sigma="true" avg="true" max="true" min="true"/>
            </Measurement>
        </Channel>
    </Channels>
    <Desktop>
        <property value="true" name="table_tab"/>
        <property value="true" name="digital_tab"/>
        <property value="true" name="analog_tab"/>
        <property value="false" name="voltage_per_cell_tab"/>
        <property description="UTILITY_GRAPHICS_TAB" value="false" name="utility_garphics_tab"/>
        <property description="UTILITY_DEVICE_TAB" value="false" name="utility_device_tab"/>
    </Desktop>
</DeviceProperties>
