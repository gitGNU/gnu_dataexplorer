<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DeviceProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DeviceProperties_V27.xsd">
    <Device>
        <name implementation="gde.device.graupner.HoTTViewer">HoTTViewer</name>
        <manufacturer>Graupner</manufacturer>
        <manufacturerURL>http://www.graupner.de</manufacturerURL>
        <group>logger</group>
        <image>HoTTViewer.png</image>
        <usage>false</usage>
    </Device>
    <TimeBase>
        <name>Time</name>
        <symbol>t</symbol>
        <unit>msec</unit>
        <timeStep>-1.0</timeStep>
        <UTCdelta>0</UTCdelta>
    </TimeBase>
    <DataBlock>
        <format inputType="FILE_IO" formatType="BYTE" size="64"/>
        <timeUnit>msec</timeUnit>
        <leader>$</leader>
        <trailer>0D0A</trailer>
        <preferredDataLocation></preferredDataLocation>
        <preferredFileExtention>*.bin</preferredFileExtention>
    </DataBlock>
    <State>
        <property name="Datenaufzeichnung" value="01" type="Integer" description=""/>
    </State>
    <Channels lastUseOrdinal="0">
        <Channel name="Allgemein" type="TYPE_CONFIG">
            <Measurement>
                <name>RXSQ</name>
                <symbol></symbol>
                <unit>%</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="true"/>
            </Measurement>
            <Measurement>
                <name>SpannungRx</name>
                <symbol>U</symbol>
                <unit>V</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="true"/>
            </Measurement>
            <Measurement>
                <name>TemperaturRx</name>
                <symbol>T</symbol>
                <unit>°C</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Steigen 1</name>
                <symbol>vh</symbol>
                <unit>m/sec</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="false" sigma="false" countByTrigger="true" countTriggerText="Anzahl gewerteter Steigflüge" sumTriggerTimeText="~ Motorlaufzeit">
                    <trigger level="1500" isGreater="true" minTimeSec="10" comment="Trigger: Steigrate &gt; 1.5 m/sec &gt; 10 sec"/>
                </statistics>
            </Measurement>
            <Measurement>
                <name>Steigen 3</name>
                <symbol>vh</symbol>
                <unit>m/3sec</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="false" sigma="false"/>
                <property name="scale_sync_ref_ordinal" value="3" type="Integer" description="Skalasynchronisations Messwertreferenz als Zahl"/>
            </Measurement>
            <Measurement>
                <name>Steigen 10</name>
                <symbol>vh</symbol>
                <unit>m/10sec</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="false" sigma="false"/>
                <property name="scale_sync_ref_ordinal" value="3" type="Integer" description="Skalasynchronisations Messwertreferenz als Zahl"/>
            </Measurement>
            <Measurement>
                <name>Höhe (relativ)</name>
                <symbol>h</symbol>
                <unit>m</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="false" sigma="false" triggerRefOrdinal="10" sumByTriggerRefOrdinal="10" sumTriggerText="Summe Motorhöhe"/>
            </Measurement>
            <Measurement>
                <name>Geschwindigkeit</name>
                <symbol>v</symbol>
                <unit>km/h</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Drehzahl</name>
                <symbol>rpm</symbol>
                <unit>1/min</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Spannung</name>
                <symbol>U</symbol>
                <unit>V</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Strom</name>
                <symbol>I</symbol>
                <unit>A</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>           
            </Measurement>
            <Measurement>
                <name>Kapazität</name>
                <symbol>C</symbol>
                <unit>mAh</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="false" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Leistung</name>
                <symbol>P</symbol>
                <unit>W</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Kraftstoff</name>
                <symbol></symbol>
                <unit>ml</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="false" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>ZellenBalance</name>
                <symbol></symbol>
                <unit>mV</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>ZellenMittel</name>
                <symbol>U</symbol>
                <unit>V</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="false" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Temperatur 1</name>
                <symbol>T</symbol>
                <unit>°C</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Temperatur 2</name>
                <symbol>T</symbol>
                <unit>°C</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
                <property name="scale_sync_ref_ordinal" value="16" type="Integer" description="Skalasynchronisations Messwertreferenz als Zahl"/>
            </Measurement>
            <Measurement>
                <name>Spannung S1</name>
                <symbol>U</symbol>
                <unit>V</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Spannung S2</name>
                <symbol>U</symbol>
                <unit>V</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
                <property name="scale_sync_ref_ordinal" value="18" type="Integer" description="Skalasynchronisations Messwertreferenz als Zahl"/>
            </Measurement>
            <Measurement>
                <name>EntfernungStart</name>
                <symbol></symbol>
                <unit>m</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Kurswinkel</name>
                <symbol></symbol>
                <unit>°</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Breitengrad</name>
                <symbol></symbol>
                <unit>° '</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Längengrad</name>
                <symbol></symbol>
                <unit>° '</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>SpannungTx</name>
                <symbol>U</symbol>
                <unit>V</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Rundenzeit</name>
                <symbol>t</symbol>
                <unit>Sek</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="false" sigma="false"/>
            </Measurement>
        </Channel>
    </Channels>
    <Desktop>
        <property name="table_tab" value="true"/>
        <property name="digital_tab" value="true"/>
        <property name="analog_tab" value="true"/>
        <property name="voltage_per_cell_tab" value="false"/>
        <property name="utility_garphics_tab" value="false" description="UTILITY_GRAPHICS_TAB"/>
        <property name="utility_device_tab" value="true" description="UTILITY_DEVICE_TAB"/>
    </Desktop>
</DeviceProperties>
