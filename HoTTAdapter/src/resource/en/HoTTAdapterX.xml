<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DeviceProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DeviceProperties_V26.xsd">
    <Device>
        <name implementation="gde.device.graupner.HoTTAdapterX">HoTTAdapterX</name>
        <manufacturer>Graupner</manufacturer>
        <manufacturerURL>http://www.graupner.de</manufacturerURL>
        <group>logger</group>
        <image>HoTTAdapterX.png</image>
        <usage>false</usage>
    </Device>
    <TimeBase>
        <name>Time</name>
        <symbol>t</symbol>
        <unit>msec</unit>
        <timeStep>-1.0</timeStep>
        <UTCdelta>0</UTCdelta>
    </TimeBase>
    <DataBlock>
        <format size="64" formatType="BYTE" inputType="FILE_IO"/>
        <timeUnit>msec</timeUnit>
        <preferredDataLocation></preferredDataLocation>
        <preferredFileExtention>*.bin</preferredFileExtention>
    </DataBlock>
    <State>
        <property description="" type="Integer" value="01" name="data recording"/>
    </State>
    <Channels>
        <Channel type="TYPE_CONFIG" name="Receiver">
            <Measurement>
                <name>Rx->Tx-PLoss</name>
                <symbol></symbol>
                <unit>%</unit>
                <active>true</active>
            </Measurement>
            <Measurement>
                <name>RXSQ</name>
                <symbol></symbol>
                <unit>%</unit>
                <active>true</active>
                <statistics sigma="true" avg="true" max="true" min="true"/>
            </Measurement>
            <Measurement>
                <name>Strength</name>
                <symbol></symbol>
                <unit>%</unit>
                <active>true</active>
                <statistics sigma="true" avg="true" max="true" min="true"/>
            </Measurement>
            <Measurement>
                <name>VPack</name>
                <symbol></symbol>
                <unit>ms</unit>
                <active>true</active>
                <statistics sigma="false" avg="false" max="true" min="true"/>
            </Measurement>
            <Measurement>
                <name>Tx</name>
                <symbol></symbol>
                <unit>dBm</unit>
                <active>true</active>
                <statistics sigma="true" avg="true" max="true" min="true"/>
            </Measurement>
            <Measurement>
                <name>Rx</name>
                <symbol></symbol>
                <unit>dBm</unit>
                <active>true</active>
                <statistics sigma="true" avg="true" max="true" min="true"/>
                <property description="Scale synchronisazion measurement reference as ordinal value" type="Integer" value="4" name="scale_sync_ref_ordinal"/>
            </Measurement>
            <Measurement>
                <name>VoltageRx</name>
                <symbol>Urx</symbol>
                <unit>V</unit>
                <active>true</active>
                <statistics sigma="true" avg="true" max="true" min="true"/>
                <property description="Factor to a measured value" type="Double" value="0.1" name="factor"/>
            </Measurement>
            <Measurement>
                <name>TemperatureRx</name>
                <symbol>Trx</symbol>
                <unit>°C</unit>
                <active>true</active>
                <statistics sigma="false" avg="true" max="true" min="true"/>
                <property description="Reduction of a measured value subtracted before applying offset or factor" type="Double" value="20.0" name="reduction"/>
            </Measurement>
            <Measurement>
                <name>VoltageRx_min</name>
                <symbol>Umin</symbol>
                <unit>V</unit>
                <active>true</active>
                <statistics sigma="false" avg="false" max="true" min="true"/>
                <property description="Faktor zu einem gemessenem Wert" type="Double" value="0.1" name="factor"/>
                <property description="Skalasynchronisations Messwertreferenz als Zahl" type="Integer" value="6" name="scale_sync_ref_ordinal"/>
            </Measurement>
        </Channel>
        <Channel type="TYPE_CONFIG" name="Channels">
            <Measurement>
                <name>FreqCh</name>
                <symbol></symbol>
                <unit></unit>
                <active>true</active>
                <statistics sigma="false" avg="true" max="true" min="true"/>
            </Measurement>
            <Measurement>
                <name>Tx</name>
                <symbol></symbol>
                <unit>dBm</unit>
                <active>true</active>
                <statistics sigma="true" avg="true" max="true" min="true"/>
            </Measurement>
            <Measurement>
                <name>Rx</name>
                <symbol></symbol>
                <unit>dBm</unit>
                <active>true</active>
                <statistics sigma="true" avg="true" max="true" min="true"/>
                <property description="Skalasynchronisations Messwertreferenz als Zahl" type="Integer" value="1" name="scale_sync_ref_ordinal"/>
            </Measurement>
            <Measurement>
                <name>Ch 1</name>
                <symbol></symbol>
                <unit>µsec</unit>
                <active>true</active>
                <statistics sigma="false" avg="true" max="true" min="true"/>
            </Measurement>
            <Measurement>
                <name>Ch 2</name>
                <symbol></symbol>
                <unit>µsec</unit>
                <active>true</active>
                <statistics sigma="false" avg="true" max="true" min="true"/>
                <property description="Skalasynchronisations Messwertreferenz als Zahl" type="Integer" value="3" name="scale_sync_ref_ordinal"/>
            </Measurement>
            <Measurement>
                <name>Ch 3</name>
                <symbol></symbol>
                <unit>µsec</unit>
                <active>true</active>
                <statistics sigma="false" avg="true" max="true" min="true"/>
                <property description="Skalasynchronisations Messwertreferenz als Zahl" type="Integer" value="3" name="scale_sync_ref_ordinal"/>
            </Measurement>
            <Measurement>
                <name>Ch 4</name>
                <symbol></symbol>
                <unit>µsec</unit>
                <active>true</active>
                <statistics sigma="false" avg="true" max="true" min="true"/>
                <property description="Skalasynchronisations Messwertreferenz als Zahl" type="Integer" value="3" name="scale_sync_ref_ordinal"/>
            </Measurement>
            <Measurement>
                <name>Ch 5</name>
                <symbol></symbol>
                <unit>µsec</unit>
                <active>true</active>
                <statistics sigma="false" avg="true" max="true" min="true"/>
                <property description="Skalasynchronisations Messwertreferenz als Zahl" type="Integer" value="3" name="scale_sync_ref_ordinal"/>
            </Measurement>
            <Measurement>
                <name>Ch 6</name>
                <symbol></symbol>
                <unit>µsec</unit>
                <active>true</active>
                <statistics sigma="false" avg="true" max="true" min="true"/>
                <property description="Skalasynchronisations Messwertreferenz als Zahl" type="Integer" value="3" name="scale_sync_ref_ordinal"/>
            </Measurement>
            <Measurement>
                <name>Ch 7</name>
                <symbol></symbol>
                <unit>µsec</unit>
                <active>true</active>
                <statistics sigma="false" avg="true" max="true" min="true"/>
                <property description="Skalasynchronisations Messwertreferenz als Zahl" type="Integer" value="3" name="scale_sync_ref_ordinal"/>
            </Measurement>
            <Measurement>
                <name>Ch 8</name>
                <symbol></symbol>
                <unit>µsec</unit>
                <active>true</active>
                <statistics sigma="false" avg="true" max="true" min="true"/>
                <property description="Skalasynchronisations Messwertreferenz als Zahl" type="Integer" value="3" name="scale_sync_ref_ordinal"/>
            </Measurement>
            <Measurement>
                <name>Power Off</name>
                <symbol>evt</symbol>
                <unit></unit>
                <active>false</active>
                <statistics sigma="false" avg="false" max="true" min="true"/>
            </Measurement>
            <Measurement>
                <name>Batt Low</name>
                <symbol>evt</symbol>
                <unit></unit>
                <active>false</active>
                <statistics sigma="false" avg="false" max="true" min="true"/>
            </Measurement>
            <Measurement>
                <name>Reset</name>
                <symbol>evt</symbol>
                <unit></unit>
                <active>false</active>
                <statistics sigma="false" avg="false" max="true" min="true"/>
            </Measurement>
            <Measurement>
                <name>Reserve</name>
                <symbol>evt</symbol>
                <unit></unit>
                <active>false</active>
                <statistics sigma="false" avg="false" max="true" min="true"/>
            </Measurement>
        </Channel>
        <property type="Boolean" value="true" name="enable_filter"/>
    </Channels>
    <Desktop>
        <property value="true" name="table_tab"/>
        <property value="true" name="digital_tab"/>
        <property value="true" name="analog_tab"/>
        <property value="false" name="voltage_per_cell_tab"/>
        <property description="UTILITY_GRAPHICS_TAB" value="false" name="utility_garphics_tab"/>
        <property description="UTILITY_DEVICE_TAB" value="true" name="utility_device_tab"/>
    </Desktop>
</DeviceProperties>
