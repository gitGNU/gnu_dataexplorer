<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DeviceProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DeviceProperties_V29.xsd">
    <Device>
        <name implementation="gde.device.graupner.HoTTViewer">HoTTViewer</name>
        <manufacturer>Graupner</manufacturer>
        <manufacturerURL>http://www.graupner.de</manufacturerURL>
        <group>logger</group>
        <image>HoTTViewer.png</image>
        <usage>false</usage>
    </Device>
    <TimeBase>
        <name>Time</name>
        <symbol>t</symbol>
        <unit>msec</unit>
        <timeStep>-1.0</timeStep>
        <UTCdelta>0</UTCdelta>
    </TimeBase>
    <DataBlock>
        <format inputType="FILE_IO" formatType="BYTE" size="64"/>
        <timeUnit>msec</timeUnit>
        <leader>$</leader>
        <trailer>0D0A</trailer>
        <preferredDataLocation></preferredDataLocation>
        <preferredFileExtention>*.bin</preferredFileExtention>
    </DataBlock>
    <State>
        <property name="Data Record" value="01" type="Integer" description=""/>
    </State>
    <Channels lastUseOrdinal="0">
        <Channel name="Standard" type="TYPE_CONFIG">
            <Measurement>
                <name>RXSQ</name>
                <symbol></symbol>
                <unit>%</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="true"/>
            </Measurement>
            <Measurement>
                <name>VoltageRx</name>
                <symbol>U</symbol>
                <unit>V</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="true"/>
            </Measurement>
            <Measurement>
                <name>TemperatureRx</name>
                <symbol>T</symbol>
                <unit>°C</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Climb 1</name>
                <symbol>vh</symbol>
                <unit>m/sec</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="false" sigma="false" countByTrigger="true" countTriggerText="Anzahl gewerteter Steigflüge" sumTriggerTimeText="~ Motorlaufzeit">
                    <trigger level="1500" isGreater="true" minTimeSec="10" comment="Trigger: Steigrate &gt; 1.5 m/sec &gt; 10 sec"/>
                </statistics>
            </Measurement>
            <Measurement>
                <name>Climb 3</name>
                <symbol>vh</symbol>
                <unit>m/3sec</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="false" sigma="false"/>
                <property name="scale_sync_ref_ordinal" value="3" type="Integer" description="Skalasynchronisations Messwertreferenz als Zahl"/>
            </Measurement>
            <Measurement>
                <name>Climb 10</name>
                <symbol>vh</symbol>
                <unit>m/10sec</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="false" sigma="false"/>
                <property name="scale_sync_ref_ordinal" value="3" type="Integer" description="Skalasynchronisations Messwertreferenz als Zahl"/>
            </Measurement>
            <Measurement>
                <name>Height (relative)</name>
                <symbol>h</symbol>
                <unit>m</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="false" sigma="false" triggerRefOrdinal="10" sumByTriggerRefOrdinal="10" sumTriggerText="Summe Motorhöhe"/>
            </Measurement>
            <Measurement>
                <name>Speed</name>
                <symbol>v</symbol>
                <unit>km/h</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Revolution</name>
                <symbol>rpm</symbol>
                <unit>1/min</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Voltage</name>
                <symbol>U</symbol>
                <unit>V</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Current</name>
                <symbol>I</symbol>
                <unit>A</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>           
            </Measurement>
            <Measurement>
                <name>Capacity</name>
                <symbol>C</symbol>
                <unit>mAh</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="false" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Power</name>
                <symbol>P</symbol>
                <unit>W</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Fuel</name>
                <symbol></symbol>
                <unit>ml</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="false" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>VoltageBalanceCell</name>
                <symbol></symbol>
                <unit>mV</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>VoltageAverageCell</name>
                <symbol>U</symbol>
                <unit>V</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="false" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Temperature 1</name>
                <symbol>T</symbol>
                <unit>°C</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Temperature 2</name>
                <symbol>T</symbol>
                <unit>°C</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
                <property name="scale_sync_ref_ordinal" value="16" type="Integer" description="Skalasynchronisations Messwertreferenz als Zahl"/>
            </Measurement>
            <Measurement>
                <name>Voltage S1</name>
                <symbol>U</symbol>
                <unit>V</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Voltage S2</name>
                <symbol>U</symbol>
                <unit>V</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
                <property name="scale_sync_ref_ordinal" value="18" type="Integer" description="Skalasynchronisations Messwertreferenz als Zahl"/>
            </Measurement>
            <Measurement>
                <name>DistanceStart</name>
                <symbol></symbol>
                <unit>m</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Azimuth</name>
                <symbol></symbol>
                <unit>°</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Latitude</name>
                <symbol></symbol>
                <unit>° '</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Longitude</name>
                <symbol></symbol>
                <unit>° '</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>VoltageTx</name>
                <symbol>U</symbol>
                <unit>V</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Lap Time</name>
                <symbol>t</symbol>
                <unit>sec</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="false" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Rx</name>
                <symbol>rx</symbol>
                <unit>dbm</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="true"/>
            </Measurement>
        </Channel>
    </Channels>
    <Desktop>
        <property name="table_tab" value="true"/>
        <property name="digital_tab" value="true"/>
        <property name="analog_tab" value="true"/>
        <property name="voltage_per_cell_tab" value="false"/>
        <property name="utility_garphics_tab" value="false" description="UTILITY_GRAPHICS_TAB"/>
        <property name="utility_device_tab" value="true" description="UTILITY_DEVICE_TAB"/>
    </Desktop>
</DeviceProperties>
