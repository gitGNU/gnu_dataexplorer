#!/bin/bash
# ***** script to launch DataExplorer ** 27 Apr 2009 WB *****
#fix Ubuntu Unity problem not displaying sliders set LIBOVERLAY_SCROLLBAR to 0
export LIBOVERLAY_SCROLLBAR=0

echo java -Dfile.encoding=UTF-8 -jar -Xms64m -Xmx1024m ./DataExplorer.jar
echo argument $1
java -Dfile.encoding=UTF-8 -jar -Xms64m -Xmx1024m ./DataExplorer.jar $1

