#!/bin/bash
# ***** script to launch DataExplorer ** 27 Apr 2009 WB *****

# determine and switch to the resource directory of the application bundle
OSDE_DIR="$(dirname ${0})/../Resources"
cd ${OSDE_DIR}

echo java -XstartOnFirstThread -d32 -Dfile.encoding=UTF-8 -jar -Xms32m -Xmx256m ./DataExplorer.jar
java -XstartOnFirstThread -d32 -Dfile.encoding=UTF-8 -jar -Xms32m -Xmx256m ./DataExplorer.jar

