# udev rule to make SkyRC M3000 USB HID device usable by users of group uucp 
# which is required for serial communication as well              
#SUBSYSTEM=="usb", ATTRS{idVendor}=="0000", ATTRS{idProduct}=="0001", MODE="0666", GROUP="uucp"
#SUBSYSTEM=="usb_device", ATTRS{idVendor}=="0000", ATTRS{idProduct}=="0001", MODE="0666", GROUP="uucp"
SUBSYSTEM=="usb", ENV{DEVTYPE}=="usb_device",SYSFS{idVendor}=="0000" , SYSFS{idProduct}=="0001", MODE="0666", GROUP="uucp"
