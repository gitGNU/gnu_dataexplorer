#!/bin/bash
# ***** script to launch DevicePropertiesEditor ** 27 Jan 2010 WB *****

# determine and switch to the resource directory of the application bundle
OSDE_DIR="$(dirname ${0})/../Resources"
cd ${OSDE_DIR}

echo java -XstartOnFirstThread -Dfile.encoding=UTF-8  -Xms10m -Xmx56m -classpath ./OpenSerialDataExplorer.jar  osde.ui.dialog.edit.DevicePropertiesEditor
java -XstartOnFirstThread -Dfile.encoding=UTF-8 -Xms10m -Xmx56m -classpath ./OpenSerialDataExplorer.jar  osde.ui.dialog.edit.DevicePropertiesEditor

