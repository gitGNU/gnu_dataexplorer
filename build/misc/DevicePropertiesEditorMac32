#!/bin/bash
# ***** script to launch DevicePropertiesEditor ** 27 Jan 2010 WB *****

# determine and switch to the resource directory of the application bundle
DE_DIR="$(dirname ${0})/../Resources"
cd ${DE_DIR}

echo java -XstartOnFirstThread -d32 -Dfile.encoding=UTF-8  -Xms10m -Xmx56m -classpath ./DataExplorer.jar  osde.ui.dialog.edit.DevicePropertiesEditor
java -XstartOnFirstThread -d32 -Dfile.encoding=UTF-8  -Xms10m -Xmx56m -classpath ./DataExplorer.jar  osde.ui.dialog.edit.DevicePropertiesEditor

