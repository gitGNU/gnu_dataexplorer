#!/bin/bash
# ***** script to launch DevicePropertiesEditor ** 27 Jan 2010 WB *****

echo java -Dfile.encoding=UTF-8 -Xms32m -Xmx256m -classpath ./DataExplorer.jar gde.ui.dialog.edit.DevicePropertiesEditor
java -Dfile.encoding=UTF-8 -Xms32m -Xmx256m -classpath ./DataExplorer.jar gde.ui.dialog.edit.DevicePropertiesEditor

