y description="Subtract last height value from preceding" type="Boolean" value="false" name="do_subtract_last"/>				<property description="Subtract offset from height values" type="Boolean" value="false" name="do_offset_height"/>				<property description="Height offset" type="Double" value="0.0" name="offset"/>			</Measurement>			<Measurement>				<name>Climb</name>				<symbol>Vc</symbol>				<unit>m/sec</unit>				<statistics sigma="false" avg="true" max="true" min="true"/>				<property description="Zeit in Sekunden �ie die regression durchgef�ird" type="Integer" value="10" name="regression_interval_sec"/>				<property description="Regression calculation type" type="String" value="regression_type_curve" name="regression_type"/>			</Measurement>		</Channel>		<Channel type="TYPE_CONFIG" name="Motor">			<Measurement>				<name>Voltage</name>				<symbol>U</symbol>				<unit>V</unit>				<active>true</active>				<statistics sigma="true" avg="true" max="true" min="true"/>				<property description="Voltage offset" type="Double" value="2.5" name="offset"/>				<property description="Value reduction" type="Double" value="45" name="reduction"/>				<property description="Voltage factor" type="Double" value="0.0532" name="factor"/>			</Measurement>			<Measurement>				<name>Height</name>				<symbol>H</symbol>				<unit>m</unit>				<active>true</active>				<statistics countTriggerText="Calculated within slope trigger ranges" sumTriggerText="Sum motor climb height" sumByTriggerRefOrdinal="2" sigma="false" avg="false" max="true" min="true"/>				<property description="Height calculation Feet to Meter  (1852 / 6076)" type="Double" value="0.304806" name="factor"/>				<property description="Do not adapt height values" type="Boolean" value="false" name="do_no_adaption"/>				<property description="Subtract first height value from subsequent" type="Boolean" value="false" name="do_subtract_first"/>				<property description="Subtract last height value from preceding" type="Boolean" value="true" name="subtract_last"/>				<property description="Subtract offset from height values" type="Boolean" value="false" name="do_offset_height"/>				<property description="Height offset" type="Double" value="0.0" name="offset"/>			</Measurement>			<Measurement>				<name>Climb</name>				<symbol>Vc</symbol>				<unit>m/sec</unit>				<statistics countTriggerText="Number voted climbs" countByTrigger="true" sigma="true" avg="true" max="true" min="false" sumTriggerTimeText="~ motor run time">					<trigger comment="Trigger: Slope &gt; 1 m/sec &gt; 15 sec" minTimeSec="15" isGreater="true" level="1000"/>				</statistics>				<property description="Zeit in Sekunden �ie die regression durchgef�ird" type="Integer" value="10" name="regression_interval_sec"/>				<property description="Regression calculation type" type="String" value="regression_type_curve" name="regression_type"/>			</Measurement>		</Channel>	</Channels>	<Desktop>		<property value="false" name="table_tab"/>		<property value="false" name="voltage_per_cell_tab"/>		<property value="false" name="analog_tab"/>		<property value="false" name="digital_tab"/>	</Desktop></DeviceProperties>