<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DeviceProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DeviceProperties_V21.xsd">
    <Device>
        <name>Picolario2</name>
        <manufacturer>Renschler</manufacturer>
        <manufacturerURL>http://www.renschler.de</manufacturerURL>
        <group>logger</group>
        <image>Picolario.jpg</image>
        <usage>true</usage>
    </Device>
    <TimeBase>
        <name>Zeit</name>
        <symbol>t</symbol>
        <unit>msec</unit>
        <timeStep>-1.0</timeStep>
    </TimeBase>
    <DataBlock>
        <format size="64" formatType="BYTE" inputType="FILE_IO"/>
        <timeUnit>sec</timeUnit>
        <leader>$</leader>
        <trailer>0D0A</trailer>
        <preferredDataLocation></preferredDataLocation>
        <preferredFileExtention>*.p2l</preferredFileExtention>
    </DataBlock>
    <State>
        <property description="" type="Integer" value="01" name="Datenaufzeichnung"/>
    </State>
    <Channels>
        <Channel type="TYPE_CONFIG" name="Thermik">
            <Measurement>
                <name>Höhe</name>
                <symbol>h</symbol>
                <unit>m</unit>
                <active>true</active>
                <statistics sigma="false" avg="true" max="true" min="true"/>
                <property description="Reduktion aller gemessenem Werte durch den ersten Messwert" type="Boolean" value="true" name="do_subtract_first"/>
            </Measurement>
            <Measurement>
                <name>Druck</name>
                <symbol>p</symbol>
                <unit>mbar</unit>
                <active>true</active>
                <statistics sigma="false" avg="true" max="true" min="true"/>
                <property description="Faktor zu einem gemessenem Wert" type="Double" value="0.01" name="factor"/>
            </Measurement>
            <Measurement>
                <name>SpannungRx</name>
                <symbol>Urx</symbol>
                <unit>V</unit>
                <active>true</active>
                <statistics sigma="true" avg="true" max="true" min="true"/>
                <property description="Faktor zu einem gemessenem Wert" type="Double" value="0.0038" name="factor"/>
            </Measurement>
            <Measurement>
                <name>Steigen</name>
                <symbol>V</symbol>
                <unit>m/s</unit>
                <active>true</active>
                <statistics sigma="false" avg="false" max="true" min="true"/>
                <property description="Zeit in Sekunden über die die regression durchgeführt wird" type="Integer" value="5" name="regression_interval_sec"/>
                <property description="Faktor zu einem gemessenem Wert" type="Double" value="100.0" name="factor"/>
                <property description="Benutze die nicht-lineare Regressionsanalyse zur Kurvenglättung" type="String" value="regression_type_curve" name="regression_type"/>
            </Measurement>
            <Measurement>
                <name>Spannung</name>
                <symbol>U</symbol>
                <unit>V</unit>
                <active>true</active>
                <statistics sigma="true" avg="true" max="true" min="true"/>
            </Measurement>
            <Measurement>
                <name>Strom</name>
                <symbol>I</symbol>
                <unit>A</unit>
                <active>true</active>
                <statistics sigma="true" avg="true" max="true" min="true"/>
            </Measurement>
            <Measurement>
                <name>Drehzahl</name>
                <symbol></symbol>
                <unit>1/min</unit>
                <active>true</active>
                <statistics sigma="true" avg="true" max="true" min="true"/>
            </Measurement>
            <Measurement>
                <name>Temperatur</name>
                <symbol></symbol>
                <unit>°C</unit>
                <active>true</active>
                <statistics sigma="true" avg="true" max="true" min="true"/>
            </Measurement>
            <Measurement>
                <name>Speed (GPS)</name>
                <symbol>v</symbol>
                <unit>km/h</unit>
                <active>true</active>
                <statistics sigma="false" avg="false" max="true" min="true"/>
            </Measurement>
            <Measurement>
                <name>Breitengrad</name>
                <symbol>B</symbol>
                <unit>°</unit>
                <active>true</active>
            </Measurement>
            <Measurement>
                <name>Längengrad</name>
                <symbol>L</symbol>
                <unit>°</unit>
                <active>true</active>
            </Measurement>
            <Measurement>
                <name>Höhe (GPS)</name>
                <symbol>h</symbol>
                <unit>m</unit>
                <active>true</active>
                <statistics sigma="false" avg="false" max="true" min="true"/>
            </Measurement>
        </Channel>
        <Channel type="TYPE_CONFIG" name="Motor">
            <Measurement>
                <name>Spannung</name>
                <symbol>U</symbol>
                <unit>V</unit>
                <active>true</active>
                <statistics sigma="true" avg="true" max="true" min="true"/>
                <property description="Spannungsoffset" type="Double" value="2.5" name="offset"/>
                <property description="Messwertreduktion" type="Double" value="45" name="reduction"/>
                <property description="Spannungsfaktor" type="Double" value="0.0532" name="factor"/>
            </Measurement>
            <Measurement>
                <name>Höhe</name>
                <symbol>H</symbol>
                <unit>m</unit>
                <active>true</active>
                <statistics countTriggerText="Ermittelt im Steigungstriggerbereich" sumTriggerText="Summe Motorhöhe" sumByTriggerRefOrdinal="2" sigma="false" avg="false" max="true" min="true"/>
                <property description="Höhenwertberechnung von Feet in Meter  (1852 / 6076)" type="Double" value="0.304806" name="factor"/>
                <property description="Höhenwerte nicht anpassen" type="Boolean" value="false" name="do_no_adaption"/>
                <property description="ersten Höhenwert von den Restlichen abziehen" type="Boolean" value="false" name="do_subtract_first"/>
                <property description="letzten Höhenwert von den Restlichen abziehen" type="Boolean" value="true" name="subtract_last"/>
                <property description="Offset von Höhenwerten abziehen" type="Boolean" value="false" name="do_offset_height"/>
                <property description="Höhenoffsetwert" type="Double" value="0.0" name="offset"/>
            </Measurement>
            <Measurement>
                <name>Steigen</name>
                <symbol>V</symbol>
                <unit>m/s</unit>
                <statistics sumTriggerTimeText="~ Motorlaufzeit" countTriggerText="Anzahl gewerteter Steigflüge" countByTrigger="true" sigma="true" avg="true" max="true" min="false">
                    <trigger comment="Trigger: Steigrate &gt; 1 m/sec &gt; 15 sec" minTimeSec="15" isGreater="true" level="1000"/>
                </statistics>
                <property description="Zeit in Sekunden über die die regression durchgeführt wird" type="Integer" value="10" name="regression_interval_sec"/>
                <property description="Regressionsrechnungsart" type="String" value="regression_type_curve" name="regression_type"/>
            </Measurement>
        </Channel>
    </Channels>
    <Desktop>
        <property value="true" name="table_tab"/>
        <property value="false" name="voltage_per_cell_tab"/>
        <property value="true" name="analog_tab"/>
        <property value="false" name="digital_tab"/>
        <property description="UTILITY_GRAPHICS_TAB" value="false" name="utility_garphics_tab"/>
        <property description="UTILITY_DEVICE_TAB" value="false" name="utility_device_tab"/>
    </Desktop>
</DeviceProperties>
