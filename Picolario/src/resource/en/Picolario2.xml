<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DeviceProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DeviceProperties_V23.xsd">
    <Device>
        <name>Picolario2</name>
        <manufacturer>Renschler</manufacturer>
        <manufacturerURL>http://www.renschler.de</manufacturerURL>
        <group>logger</group>
        <image>Picolario.jpg</image>
        <usage>false</usage>
    </Device>
    <TimeBase>
        <name>Time</name>
        <symbol>t</symbol>
        <unit>msec</unit>
        <timeStep>-1.0</timeStep>
    </TimeBase>
    <DataBlock>
        <format inputType="FILE_IO" formatType="BYTE" size="64"/>
        <preferredDataLocation></preferredDataLocation>
        <preferredFileExtention>*.p2l</preferredFileExtention>
    </DataBlock>
    <State>
        <property name="flight record" value="01" type="Integer" description=""/>
    </State>
    <Channels lastUseOrdinal="1">
        <Channel name="Thermal" type="TYPE_CONFIG">
            <Measurement>
                <name>Height</name>
                <symbol>h</symbol>
                <unit>m</unit>
                <active>true</active>
  				<statistics countTriggerText="Calculated within slope trigger ranges" sumTriggerText="Sum motor climb height" sumByTriggerRefOrdinal="3" sigma="false" avg="false" max="true" min="true"/>
				<property description="Subtract first height value from subsequent" type="Boolean" value="true" name="do_subtract_first"/>
            </Measurement>
            <Measurement>
                <name>Pressure</name>
                <symbol>p</symbol>
                <unit>mbar</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
                <property name="factor" value="0.01" type="Double" description="Measurement factor"/>
            </Measurement>
            <Measurement>
                <name>VoltageRx</name>
                <symbol>Urx</symbol>
                <unit>V</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="true"/>
                <property name="factor" value="0.0038" type="Double" description="Measurement factor"/>
            </Measurement>
            <Measurement>
                <name>Climb</name>
                <symbol>Vh</symbol>
                <unit>m/s</unit>
                <statistics min="false" max="true" avg="true" sigma="true" countByTrigger="true" countTriggerText="Number climb flights" sumTriggerTimeText="~ Motor run time">
                    <trigger level="1000" isGreater="true" minTimeSec="15" comment="Trigger: climb rate &gt; 1 m/sec &gt; 15 sec"/>
                </statistics>
            </Measurement>
            <Measurement>
                <name>Latitude</name>
                <symbol></symbol>
                <unit>°</unit>
                <active>true</active>
            </Measurement>
            <Measurement>
                <name>Longitude</name>
                <symbol>L</symbol>
                <unit>°</unit>
                <active>true</active>
            </Measurement>
            <Measurement>
                <name>Altitude (GPS)</name>
                <symbol>h</symbol>
                <unit>m</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="false" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Speed (GPS)</name>
                <symbol>v</symbol>
                <unit>km/h</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="false" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Voltage</name>
                <symbol>U</symbol>
                <unit>V</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="true"/>
            </Measurement>
            <Measurement>
                <name>Current</name>
                <symbol>I</symbol>
                <unit>A</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="true"/>
            </Measurement>
            <Measurement>
                <name>Capacity</name>
                <symbol>c</symbol>
                <unit>mAh</unit>
                <statistics min="true" max="true" avg="true" sigma="true"/>
            </Measurement>
            <Measurement>
                <name>Power</name>
                <symbol>P</symbol>
                <unit>W</unit>
                <statistics min="true" max="true" avg="true" sigma="true"/>
            </Measurement>
            <Measurement>
                <name>Revolution</name>
                <symbol></symbol>
                <unit>1/min</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="true"/>
            </Measurement>
            <Measurement>
                <name>Temperature</name>
                <symbol></symbol>
                <unit>°C</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="true"/>
            </Measurement>
        </Channel>
        <Channel name="Motor" type="TYPE_CONFIG">
            <Measurement>
                <name>Height</name>
                <symbol>h</symbol>
                <unit>m</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false" sumByTriggerRefOrdinal="5" sumTriggerText="Motor height" comment="Referenzing motor current trigger" ratioRefOrdinal="6" ratioText="Verbrauchte Kapazität/Höhenmeter"/>
				<property description="Subtract first height value from subsequent" type="Boolean" value="true" name="do_subtract_first"/>
            </Measurement>
            <Measurement>
                <name>Pressure</name>
                <symbol>p</symbol>
                <unit>mbar</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
                <property name="factor" value="0.01" type="Double" description="Measurement factor"/>
            </Measurement>
            <Measurement>
                <name>VoltageRx</name>
                <symbol>Urx</symbol>
                <unit>V</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="true"/>
                <property name="factor" value="0.0038" type="Double" description="Measurement factor"/>
            </Measurement>
            <Measurement>
                <name>Climb</name>
                <symbol>V</symbol>
                <unit>m/s</unit>
                <statistics min="true" max="true" avg="true" sigma="false"/>
                <property name="regression_interval_sec" value="10" type="Integer" description="Regression time interval in seconds"/>
                <property name="regression_type" value="regression_type_curve" type="String" description="Regression calculation type"/>
            </Measurement>
            <Measurement>
                <name>Latitude</name>
                <symbol>B</symbol>
                <unit>°</unit>
                <active>true</active>
            </Measurement>
            <Measurement>
                <name>Longitude</name>
                <symbol>L</symbol>
                <unit>°</unit>
                <active>true</active>
            </Measurement>
            <Measurement>
                <name>Altitude (GPS)</name>
                <symbol>h</symbol>
                <unit>m</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="false" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Speed (GPS)</name>
                <symbol>v</symbol>
                <unit>km/h</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="false" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Voltage</name>
                <symbol>U</symbol>
                <unit>V</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false" triggerRefOrdinal="5" comment="Referenzing motor current trigger"/>
            </Measurement>
            <Measurement>
                <name>Current</name>
                <symbol>I</symbol>
                <unit>A</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false" countByTrigger="true" countTriggerText="Number motor climbs" sumTriggerTimeText="Motor run time">
                    <trigger level="3000" isGreater="true" minTimeSec="5" comment="Trigger motor current &gt; 3 A &gt; 5 Seconds"/>
                </statistics>
            </Measurement>
            <Measurement>
                <name>Capacity</name>
                <symbol>c</symbol>
                <unit>mAh</unit>
                <statistics min="true" max="true" avg="true" sigma="true" triggerRefOrdinal="5" comment="Referenzing motor current trigger" ratioText="verbrauchte Kapazität/Höhenmeter"/>
            </Measurement>
            <Measurement>
                <name>Power</name>
                <symbol>P</symbol>
                <unit>W</unit>
                <statistics min="true" max="true" avg="true" sigma="true" triggerRefOrdinal="5" comment="Referenzing motor current trigger"/>
            </Measurement>
            <Measurement>
                <name>Revolution</name>
                <symbol></symbol>
                <unit>1/min</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="true" triggerRefOrdinal="5" comment="Referenzing motor current trigger"/>
            </Measurement>
            <Measurement>
                <name>Temperature</name>
                <symbol></symbol>
                <unit>°C</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="true"/>
            </Measurement>
         </Channel>
    </Channels>
    <Desktop>
        <property name="table_tab" value="true"/>
        <property name="voltage_per_cell_tab" value="false"/>
        <property name="analog_tab" value="true"/>
        <property name="digital_tab" value="false"/>
        <property name="utility_garphics_tab" value="false" description="UTILITY_GRAPHICS_TAB"/>
        <property name="utility_device_tab" value="false" description="UTILITY_DEVICE_TAB"/>
    </Desktop>
</DeviceProperties>
