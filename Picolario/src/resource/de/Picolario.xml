<?xml version="1.0" encoding="UTF-8" standalone="yes"?><DeviceProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DeviceProperties_V16.xsd">	<Device>		<name>Picolario</name>		<manufacturer>Renschler</manufacturer>		<manufacturerURL>http://www.renschler.de</manufacturerURL>		<group>logger</group>		<image>Picolario.jpg</image>		<usage>false</usage>	</Device>	<SerialPort>		<port>-</port>		<baudeRate>38400</baudeRate>		<dataBits>DATABITS_8</dataBits>		<stopBits>STOPBITS_1</stopBits>		<parity>PARITY_NONE</parity>		<flowControlMode>FLOWCONTROL_NONE</flowControlMode>		<isRTS>true</isRTS>		<isDTR>false</isDTR>	</SerialPort>	<TimeBase>		<name>Zeit</name>		<symbol>t</symbol>		<unit>msec</unit>		<timeStep>50</timeStep>	</TimeBase>    <DataBlock>        <format inputType="SERIAL_IO" formatType="BYTE" size="-31"/>		<checkSum type="XOR" format="BINARY"/>    </DataBlock>    <Channels>		<Channel type="TYPE_CONFIG" name="Thermik">			<Measurement>				<name>Spannung</name>				<symbol>U</symbol>				<unit>V</unit>				<active>true</active>				<statistics sigma="true" avg="true" max="true" min="true"/>				<property description="Spannungsoffset" type="Double" value="2.5" name="offset"/>				<property description="Messwertreduktion" type="Double" value="45" name="reduction"/>				<property description="Spannungsfaktor" type="Double" value="0.0532" name="factor"/>			</Measurement>			<Measurement>				<name>Höhe</name>				<symbol>h</symbol>				<unit>m</unit>				<active>true</active>				<statistics sigma="false" avg="true" max="true" min="true"/>				<property description="Höhenwertberechnung von Feet in Meter  (1852 / 6076)" type="Double" value="0.304806" name="factor"/>				<property description="Höhenwerte nicht anpassen" type="Boolean" value="false" name="do_no_adaption"/>				<property description="ersten Höhenwert von den Restlichen abziehen" type="Boolean" value="true" name="do_subtract_first"/>				<property description="letzten Höhenwert von den Restlichen abziehen" type="Boolean" value="false" name="do_subtract_last"/>				<property description="Offset von Höhenwerten abziehen" type="Boolean" value="false" name="do_offset_height"/>				<property description="Höhenoffsetwert" type="Double" value="0.0" name="offset"/>			</Measurement>			<Measurement>				<name>Steigen</name>				<symbol>V</symbol>				<unit>m/s</unit>				<statistics sigma="false" avg="true" max="true" min="true"/>				<property description="Zeit in Sekunden über die die regression durchgeführt wird" type="Integer" value="10" name="regression_interval_sec"/>				<property description="Regressionsrechnungsart" type="String" value="regression_type_curve" name="regression_type"/>			</Measurement>		</Channel>		<Channel type="TYPE_CONFIG" name="Motor">			<Measurement>				<name>Spannung</name>				<symbol>U</symbol>				<unit>V</unit>				<active>true</active>				<statistics sigma="true" avg="true" max="true" min="true"/>				<property description="Spannungsoffset" type="Double" value="2.5" name="offset"/>				<property description="Messwertreduktion" type="Double" value="45" name="reduction"/>				<property description="Spannungsfaktor" type="Double" value="0.0532" name="factor"/>			</Measurement>			<Measurement>				<name>Höhe</name>				<symbol>H</symbol>				<unit>m</unit>				<active>true</active>				<statistics countTriggerText="Ermittelt im Steigungstriggerbereich" sumTriggerText="Summe Motorhöhe" sumByTriggerRefOrdinal="2" sigma="false" avg="false" max="true" min="true"/>				<property description="Höhenwertberechnung von Feet in Meter  (1852 / 6076)" type="Double" value="0.304806" name="factor"/>				<property description="Höhenwerte nicht anpassen" type="Boolean" value="false" name="do_no_adaption"/>				<property description="ersten Höhenwert von den Restlichen abziehen" type="Boolean" value="false" name="do_subtract_first"/>				<property description="letzten Höhenwert von den Restlichen abziehen" type="Boolean" value="true" name="subtract_last"/>				<property description="Offset von Höhenwerten abziehen" type="Boolean" value="false" name="do_offset_height"/>				<property description="Höhenoffsetwert" type="Double" value="0.0" name="offset"/>			</Measurement>			<Measurement>				<name>Steigen</name>				<symbol>V</symbol>				<unit>m/s</unit>				<statistics countTriggerText="Anzahl gewerteter Steigflüge" countByTrigger="true" sigma="true" avg="true" max="true" min="false" sumTriggerTimeText="~ Motorlaufzeit">					<trigger comment="Trigger: Steigrate &gt; 1 m/sec &gt; 15 sec" minTimeSec="15" isGreater="true" level="1000"/>				</statistics>				<property description="Zeit in Sekunden über die die regression durchgeführt wird" type="Integer" value="10" name="regression_interval_sec"/>				<property description="Regressionsrechnungsart" type="String" value="regression_type_curve" name="regression_type"/>			</Measurement>		</Channel>	</Channels>	<Desktop>		<property value="false" name="table_tab"/>		<property value="false" name="voltage_per_cell_tab"/>		<property value="false" name="analog_tab"/>		<property value="false" name="digital_tab"/>	</Desktop></DeviceProperties>